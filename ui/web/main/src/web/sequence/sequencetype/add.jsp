<%@ include file="../../include/taglibs.jsp" %>

<html:html xhtml="true">
    <head>
        <title>Types</title>
        <html:base/>
        <%@ include file="../../include/environment.jsp" %>
    </head>
    <body>
        <div id="Header">
            <h2>
                <a href="<c:url value="/action/Portal" />"><fmt:message key="navigation.portal" /></a> &gt;&gt;
                <a href="<c:url value="/action/Sequence/Main" />">Sequences</a> &gt;&gt;
                <a href="<c:url value="/action/Sequence/SequenceType/Main" />">Types</a> &gt;&gt;
                Add
            </h2>
        </div>
        <div id="Content">
            <et:executionErrors id="errorMessage">
                <p class="executionErrors"><c:out value="${errorMessage}" /></p><br />
            </et:executionErrors>
            <html:form action="/Sequence/SequenceType/Add" method="POST" focus="sequenceTypeName">
                <table>
                    <tr>
                        <td align=right><fmt:message key="label.sequenceTypeName" />:</td>
                        <td>
                            <html:text property="sequenceTypeName" size="40" maxlength="40" /> (*)
                            <et:validationErrors id="errorMessage" property="SequenceTypeName">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.prefix" />:</td>
                        <td>
                            <html:text property="prefix" size="10" maxlength="10" />
                            <et:validationErrors id="errorMessage" property="Prefix">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.suffix" />:</td>
                        <td>
                            <html:text property="suffix" size="10" maxlength="10" />
                            <et:validationErrors id="errorMessage" property="Suffix">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.sequenceEncoderType" />:</td>
                        <td>
                            <html:select property="sequenceEncoderTypeChoice">
                                <html:optionsCollection property="sequenceEncoderTypeChoices" />
                            </html:select> (*)
                            <et:validationErrors id="errorMessage" property="SequenceEncoderTypeName">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.sequenceChecksumType" />:</td>
                        <td>
                            <html:select property="sequenceChecksumTypeChoice">
                                <html:optionsCollection property="sequenceChecksumTypeChoices" />
                            </html:select> (*)
                            <et:validationErrors id="errorMessage" property="SequenceChecksumTypeName">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.chunkSize" />:</td>
                        <td>
                            <html:text property="chunkSize" size="12" maxlength="12" />
                            <et:validationErrors id="errorMessage" property="ChunkSize">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.isDefault" />:</td>
                        <td>
                            <html:checkbox property="isDefault" /> (*)
                            <et:validationErrors id="errorMessage" property="IsDefault">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.sortOrder" />:</td>
                        <td>
                            <html:text property="sortOrder" size="12" maxlength="12" /> (*)
                            <et:validationErrors id="errorMessage" property="SortOrder">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td align=right><fmt:message key="label.description" />:</td>
                        <td>
                            <html:text property="description" size="60" maxlength="132" />
                            <et:validationErrors id="errorMessage" property="Description">
                                <p><c:out value="${errorMessage}" /></p>
                            </et:validationErrors>
                        </td>
                    </tr>
                    <tr>
                        <td></td>
                        <td><html:submit onclick="onSubmitDisable(this);" /><input type="hidden" name="submitButton" /></td>
                    </tr>
                </table>
            </html:form>
        </div>
        <jsp:include page="../../include/userSession.jsp" />
        <jsp:include page="../../include/copyright.jsp" />
    </body>
</html:html>
